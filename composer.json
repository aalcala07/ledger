{
    "name": "aalcala/ledger",
    "description": "Your Package Description here",
    "type": "library",
    "license": "MIT",
    "keywords": [
		"accounting",
		"Laravel"
	],
    "authors": [
        {
            "name": "Anthony Alcala",
            "email": "anthony@aalcala.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable":true,
    "require": {},
    "require-dev": {
        "symfony/thanks": "^1.0",
        "phpunit/phpunit": "^7.4@dev",
        "mockery/mockery": "^1.0@dev",
        "orchestra/testbench": "^3.8@dev",
        "orchestra/database": "^3.8@dev",
        "illuminate/support": "^5.8@dev",
        "fzaninotto/faker": "^1.9@dev"
    },
    "autoload": {
        "psr-4": {
            "Aalcala\\Ledger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Aalcala\\Ledger\\Tests\\":"tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Aalcala\\Ledger\\LedgerServiceProvider"
            ],
            "aliases": {
                "Aalcala": "Aalcala\\Ledger\\LedgerFacade"
            }
        }
    }
}
